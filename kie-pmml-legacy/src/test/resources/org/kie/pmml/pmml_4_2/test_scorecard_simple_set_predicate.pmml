    <Characteristics>
      <Characteristic name="ch1" baselineScore="5">
        <Attribute partialScore="20">
          <SimpleSetPredicate field="param1" booleanOperator="isIn">
            <Array n="2" type="int">1 5</Array>
          </SimpleSetPredicate>
        </Attribute>
        <Attribute partialScore="100">
          <SimpleSetPredicate field="param1" booleanOperator="isNotIn">
            <Array n="2" type="int">1 5</Array>
          </SimpleSetPredicate>
        </Attribute>
      </Characteristic>

      <Characteristic name="ch2">
        <Attribute partialScore="10">
          <SimpleSetPredicate field="param2" booleanOperator="isIn">
            <Array n="2" type="string">optA optB</Array>
          </SimpleSetPredicate>
        </Attribute>
        <Attribute partialScore="20">
          <SimpleSetPredicate field="param2" booleanOperator="isNotIn">
            <Array n="2" type="string">optA optB</Array>
          </SimpleSetPredicate>
        </Attribute>
      </Characteristic>
    </Characteristics>
