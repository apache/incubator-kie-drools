package org.drools.test;

import org.drools.Cheese;

//this was missing Person
import org.drools.Person;

global java.util.List list;

rule "not rule test"
    when
        // and it had like instead of likes
        $person : Person( $likes:likes )
        not Cheese( type == $likes )
    then
        list.add( $person );
end    

