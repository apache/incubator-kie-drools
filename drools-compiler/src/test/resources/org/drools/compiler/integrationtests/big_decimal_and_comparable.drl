package org.drools.test

import org.drools.compiler.Cheese;
import org.drools.compiler.Person;
import java.math.BigDecimal;

global java.util.List list;

rule "BigDec"

    when
        Cheese($price : price)
        p : Person(bigDecimal < $price)
    then
        list.add( p );
end
