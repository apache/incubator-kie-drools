package org.drools.quarkus.ruleunit.examples.reactive

unit AlertingUnit;

rule IncomingEvent
  when
    $e : /eventData [ type == "temperature", value >= 30 ]
  then
    System.out.println("rule IncomingEvent fired : "+ $e);
    Alert alert = new Alert( "warning", $e.toString() );
    alertData.append( alert );
end
