package org.drools;

declare CallEvent 
    @role( event )
    @class( org.drools.events.Call )
    @duration( duration )
    @timestamp( timestamp )
end

declare some.pkg.Type
    @name1
    @name2( "value" )
    @name3( k1 = 10 )
    @name4( k1 = "a", formula = Math.max( 10 + 25, 22 ) % 2 + someVariable, array = { a, b, c } )
    @name5( backward compatible value )
end