                                                 id="_89fb1571-a993-46fe-a8a7-0b51e11614a0"
                                                 triso:unparsed="true">
                        <semantic:text>if n + 1 &gt; limit then
      new list
  else
      f( n+1, limit, new list )</semantic:text>
