package Some_other_business_rules;
//generated from Decision Table
import org.drools.compiler.Cheese;
import org.drools.compiler.Person;
global java.util.List list;
//From row number: 17
rule "Cheese fans_17"
 
 salience 65519
 when
  Person(age == 42)
  Cheese(type=="stilton")
 then
  list.add("Old man stilton");
end

