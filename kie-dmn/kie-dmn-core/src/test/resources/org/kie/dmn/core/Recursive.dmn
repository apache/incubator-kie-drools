                                        id="_608dfc35-d13e-459a-998b-08ce33a48ca8"
                                        triso:unparsed="true">
               <semantic:text>function( n, limit, list ) {
      new list : append( list, n ),
      result : if n + 1 &gt; limit then
                        new list
                   else
                       g( n+1, limit, new list )
  }.result</semantic:text>
            </semantic:literalExpression>
