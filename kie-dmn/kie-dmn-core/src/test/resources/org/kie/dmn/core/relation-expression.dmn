<semantic:definitions xmlns="https://github.com/kiegroup/kie-dmn"
                      xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/"
                      xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/"
                      xmlns:dmndi="https://www.omg.org/spec/DMN/20230324/DMNDI/"
                      xmlns:feel="https://www.omg.org/spec/DMN/20230324/FEEL/"
                      xmlns:semantic="https://www.omg.org/spec/DMN/20230324/MODEL/"
                      id="_relation-expression"
                      name="relation-expression"
                      namespace="https://github.com/kiegroup/kie-dmn">
   <semantic:decision name="Employee Relation" id="d_EmployeeRelation">
      <semantic:variable name="Employee Relation"/>
      <semantic:relation>
         <semantic:column name="Name" typeRef="string"/>
         <semantic:column name="Dept" typeRef="string"/>
         <semantic:column name="Salary" typeRef="number"/>
         <semantic:row>
            <semantic:literalExpression>
               <semantic:text>"John"</semantic:text>
            </semantic:literalExpression>
            <semantic:literalExpression>
               <semantic:text>"Sales"</semantic:text>
            </semantic:literalExpression>
            <semantic:literalExpression>
               <semantic:text>100000</semantic:text>
            </semantic:literalExpression>
         </semantic:row>
         <semantic:row>
            <semantic:literalExpression>
               <semantic:text>"Mary"</semantic:text>
            </semantic:literalExpression>
            <semantic:literalExpression>
               <semantic:text>"Finances"</semantic:text>
            </semantic:literalExpression>
            <semantic:literalExpression>
               <semantic:text>120000</semantic:text>
            </semantic:literalExpression>
         </semantic:row>
      </semantic:relation>
   </semantic:decision>
</semantic:definitions>
