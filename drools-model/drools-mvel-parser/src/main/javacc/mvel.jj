    [ label = SimpleName() ]
    ";"
    { return new ContinueStmt(range(begin, token()), label); }
}

/**
 * https://docs.oracle.com/javase/specs/jls/se15/html/jls-14.html#jls-14.17
 * <pre>{@code
 *     ReturnStatement:
 *         return [Expression] ;
 * }</pre>
 */
ReturnStmt ReturnStatement():
{
