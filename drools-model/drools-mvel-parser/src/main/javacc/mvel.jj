	    ">" { op = HalfBinaryExpr.Operator.GREATER; begin=token();} |
	    "<=" { op = HalfBinaryExpr.Operator.LESS_EQUALS; begin=token();} |
	    ">=" { op = HalfBinaryExpr.Operator.GREATER_EQUALS; begin=token();}
