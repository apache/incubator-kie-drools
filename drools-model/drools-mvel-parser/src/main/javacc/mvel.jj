TOKEN :
{
  < MILLISECOND_LITERAL: <DECIMAL_LITERAL> "ms" >
|
  < SECOND_LITERAL: <DECIMAL_LITERAL> "s" >
|
  < MINUTE_LITERAL: <DECIMAL_LITERAL> "m" >
|
  < HOUR_LITERAL: <DECIMAL_LITERAL> "h" >
