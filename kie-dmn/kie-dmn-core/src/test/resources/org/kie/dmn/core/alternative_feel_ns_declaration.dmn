<definitions id="0001-input-data-string" name="0001-input-data-string"
	namespace="https://github.com/kiegroup/kie-dmn"
	xmlns="https://www.omg.org/spec/DMN/20230324/MODEL/">
	<decision name="Greeting Message" id="d_GreetingMessage">
		<variable xmlns:feel="https://www.omg.org/spec/DMN/20230324/FEEL/" name="Greeting Message" typeRef="feel.string"/>
		<informationRequirement>
			<requiredInput href="#i_FullName"/>
		</informationRequirement>
		<literalExpression>
			<text>"Hello " + Full Name</text>
		</literalExpression>
	</decision>
	<inputData name="Full Name" id="i_FullName">
		<variable xmlns:feel="https://www.omg.org/spec/DMN/20230324/FEEL/" name="Full Name" typeRef="feel.string"/>
	</inputData>
</definitions>
