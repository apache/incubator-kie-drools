<FindBugsFilter>
  <!-- Ignoring following classes as they are automatically generated by ANTLR.
       It does not make any sense to fix the warnings, as they would be
       overwritten next time the classes are re-generated. Putting them to exclude
       list will slightly de-clutter the FindBugs report. -->

  <!-- The asterisk will filter out also possible inner classes (which is good) -->
  <Match>
    <Class name="~org\.drools\.compiler\.lang\.DRL5Expressions.*"/>
  </Match>
  <Match>
    <Class name="~org\.drools\.compiler\.lang\.DRL5Lexer.*"/>
  </Match>
  <Match>
    <Class name="~org\.drools\.compiler\.lang\.DRL6Expressions.*"/>
  </Match>
  <Match>
    <Class name="~org\.drools\.compiler\.lang\.DRL6Lexer.*"/>
  </Match>
  <Match>
    <Class name="~org\.drools\.compiler\.lang\.dsl\.DSLMapParser.*"/>
  </Match>
  <Match>
    <Class name="~org\.drools\.compiler\.lang\.dsl\.DSLMapWalker.*"/>
  </Match>
  <Match>
    <Class name="~org\.drools\.compiler\.lang\.dsl\.DSLMapLexer.*"/>
  </Match>

  <!-- Ignoring following classes as they are generated by protobuf and the
       changes would be overwritten once the source files are re-generated. -->
  <Match>
    <Class name="~org\.drools\.compiler\.kie\.builder\.impl\.KieModuleCache.*"/>
  </Match>
</FindBugsFilter>
