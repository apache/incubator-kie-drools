package Some_business_rules;
#generated from Decision Table
import org.drools.decisiontable.Cheese;
import org.drools.decisiontable.Person;
global java.util.List list;

template "cheesefans"
age > 21 && < 43
type
log

rule "Cheese fans_@{row.rowNumber}"
	when
		Person(age == "@{age}")
		Cheese(type == "@{type}")
	then
		list.add("@{log}");
end
end template