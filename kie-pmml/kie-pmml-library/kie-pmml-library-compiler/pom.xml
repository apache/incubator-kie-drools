<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <parent>
    <groupId>org.drools</groupId>
    <artifactId>kie-pmml-library</artifactId>
    <version>7.33.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>

  <artifactId>kie-pmml-library-compiler</artifactId>

  <name>KIE :: PMML :: Library :: Compiler </name>
  <description>PMML Compiler</description>


  <dependencies>
    <dependency>
      <groupId>org.drools</groupId>
      <artifactId>kie-pmml-api</artifactId>
    </dependency>
    <dependency>
      <groupId>org.drools</groupId>
      <artifactId>kie-pmml-library-api</artifactId>
    </dependency>
    <dependency>
      <groupId>org.jpmml</groupId>
      <artifactId>pmml-model</artifactId>
    </dependency>
    <!-- TEST -->
    <dependency>
      <groupId>org.drools</groupId>
      <artifactId>kie-pmml-library-api</artifactId>
      <classifier>tests</classifier>
      <scope>test</scope>
    </dependency>
  </dependencies>

  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>org.drools</groupId>
        <artifactId>kie-pmml-api</artifactId>
        <version>${version.org.kie}</version>
      </dependency>
      <dependency>
        <groupId>org.drools</groupId>
        <artifactId>kie-pmml-library-api</artifactId>
        <version>${version.org.kie}</version>
      </dependency>
      <!-- TEST -->
      <dependency>
        <groupId>org.drools</groupId>
        <artifactId>kie-pmml-library-api</artifactId>
        <classifier>tests</classifier>
        <scope>test</scope>
        <version>${version.org.kie}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <additionalClasspathElements>
            <additionalClasspathElement>../../kie-pmml-models/kie-pmml-models-core/target/kie-pmml-models-core-${version.org.kie}.jar</additionalClasspathElement>
            <additionalClasspathElement>../../kie-pmml-models/kie-pmml-regression/target/kie-pmml-regression-${version.org.kie}.jar</additionalClasspathElement>
          </additionalClasspathElements>
        </configuration>
      </plugin>
    </plugins>
  </build>




</project>