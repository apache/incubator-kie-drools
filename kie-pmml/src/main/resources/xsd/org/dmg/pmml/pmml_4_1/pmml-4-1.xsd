<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.dmg.org/PMML-4_1" elementFormDefault="unqualified" xmlns="http://www.dmg.org/PMML-4_1">
    <xs:element name="Taxonomy">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="ChildParent" />
            </xs:sequence>
            <xs:attribute use="required" type="xs:string" name="name" />
        </xs:complexType>
    </xs:element>
    <xs:element name="ChildParent">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:choice>
                    <xs:element ref="TableLocator" />
                    <xs:element ref="InlineTable" />
                </xs:choice>
            </xs:sequence>
            <xs:attribute use="required" type="xs:string" name="childField" />
            <xs:attribute use="required" type="xs:string" name="parentField" />
            <xs:attribute use="optional" type="xs:string" name="parentLevelField" />
            <xs:attribute default="no" use="optional" name="isRecursive">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="no" />
                        <xs:enumeration value="yes" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="TableLocator">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="InlineTable">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="row" minOccurs="0" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="row">
        <xs:complexType>
            <xs:complexContent mixed="true">
                <xs:restriction base="xs:anyType">
                    <xs:sequence>
                        <xs:any maxOccurs="unbounded" processContents="skip" minOccurs="2" />
                    </xs:sequence>
                </xs:restriction>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="MiningModel">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="MiningSchema" />
                <xs:element ref="Output" minOccurs="0" />
                <xs:element ref="ModelStats" minOccurs="0" />
                <xs:element ref="ModelExplanation" minOccurs="0" />
                <xs:element ref="Targets" minOccurs="0" />
                <xs:element ref="LocalTransformations" minOccurs="0" />
                <xs:choice maxOccurs="unbounded" minOccurs="0">
                    <xs:element ref="Regression" />
                    <xs:element ref="DecisionTree" />
                </xs:choice>
                <xs:element ref="Segmentation" minOccurs="0" />
                <xs:element ref="ModelVerification" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute use="optional" type="xs:string" name="modelName" />
            <xs:attribute use="required" type="MINING-FUNCTION" name="functionName" />
            <xs:attribute use="optional" type="xs:string" name="algorithmName" />
            <xs:attribute default="true" type="xs:boolean" name="isScorable" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Segmentation">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="Segment" />
            </xs:sequence>
            <xs:attribute use="required" type="MULTIPLE-MODEL-METHOD" name="multipleModelMethod" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Segment">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:group ref="PREDICATE" />
                <xs:group ref="MODEL-ELEMENT" />
            </xs:sequence>
            <xs:attribute use="optional" type="xs:string" name="id" />
            <xs:attribute default="1" use="optional" type="NUMBER" name="weight" />
        </xs:complexType>
    </xs:element>
    <xs:simpleType name="MULTIPLE-MODEL-METHOD">
        <xs:restriction base="xs:string">
            <xs:enumeration value="majorityVote" />
            <xs:enumeration value="weightedMajorityVote" />
            <xs:enumeration value="average" />
            <xs:enumeration value="weightedAverage" />
            <xs:enumeration value="median" />
            <xs:enumeration value="max" />
            <xs:enumeration value="sum" />
            <xs:enumeration value="selectFirst" />
            <xs:enumeration value="selectAll" />
            <xs:enumeration value="modelChain" />
        </xs:restriction>
    </xs:simpleType>
    <xs:group name="EmbeddedModel">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            <xs:choice>
                <xs:element ref="Regression" />
                <xs:element ref="DecisionTree" />
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:element name="ResultField">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute use="required" type="FIELD-NAME" name="name" />
            <xs:attribute type="xs:string" name="displayName" />
            <xs:attribute type="OPTYPE" name="optype" />
            <xs:attribute type="DATATYPE" name="dataType" />
            <xs:attribute type="RESULT-FEATURE" name="feature" />
            <xs:attribute type="xs:string" name="value" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Regression">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="Output" minOccurs="0" />
                <xs:element ref="ModelStats" minOccurs="0" />
                <xs:element ref="Targets" minOccurs="0" />
                <xs:element ref="LocalTransformations" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="ResultField" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="RegressionTable" />
            </xs:sequence>
            <xs:attribute type="xs:string" name="modelName" />
            <xs:attribute use="required" type="MINING-FUNCTION" name="functionName" />
            <xs:attribute type="xs:string" name="algorithmName" />
            <xs:attribute default="none" type="REGRESSIONNORMALIZATIONMETHOD" name="normalizationMethod" />
        </xs:complexType>
    </xs:element>
    <xs:element name="DecisionTree">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="Output" minOccurs="0" />
                <xs:element ref="ModelStats" minOccurs="0" />
                <xs:element ref="Targets" minOccurs="0" />
                <xs:element ref="LocalTransformations" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="ResultField" minOccurs="0" />
                <xs:element ref="Node" />
            </xs:sequence>
            <xs:attribute type="xs:string" name="modelName" />
            <xs:attribute use="required" type="MINING-FUNCTION" name="functionName" />
            <xs:attribute type="xs:string" name="algorithmName" />
            <xs:attribute default="none" type="MISSING-VALUE-STRATEGY" name="missingValueStrategy" />
            <xs:attribute default="1.0" type="PROB-NUMBER" name="missingValuePenalty" />
            <xs:attribute default="returnNullPrediction" type="NO-TRUE-CHILD-STRATEGY" name="noTrueChildStrategy" />
            <xs:attribute default="multiSplit" name="splitCharacteristic">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="binarySplit" />
                        <xs:enumeration value="multiSplit" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="AssociationModel">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="MiningSchema" />
                <xs:element ref="Output" minOccurs="0" />
                <xs:element ref="ModelStats" minOccurs="0" />
                <xs:element ref="LocalTransformations" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="Item" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="Itemset" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="AssociationRule" minOccurs="0" />
                <xs:element ref="ModelVerification" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute type="xs:string" name="modelName" />
            <xs:attribute use="required" type="MINING-FUNCTION" name="functionName" />
            <xs:attribute type="xs:string" name="algorithmName" />
            <xs:attribute use="required" type="INT-NUMBER" name="numberOfTransactions" />
            <xs:attribute type="INT-NUMBER" name="maxNumberOfItemsPerTA" />
            <xs:attribute type="REAL-NUMBER" name="avgNumberOfItemsPerTA" />
            <xs:attribute use="required" type="PROB-NUMBER" name="minimumSupport" />
            <xs:attribute use="required" type="PROB-NUMBER" name="minimumConfidence" />
            <xs:attribute type="INT-NUMBER" name="lengthLimit" />
            <xs:attribute use="required" type="INT-NUMBER" name="numberOfItems" />
            <xs:attribute use="required" type="INT-NUMBER" name="numberOfItemsets" />
            <xs:attribute use="required" type="INT-NUMBER" name="numberOfRules" />
            <xs:attribute default="true" type="xs:boolean" name="isScorable" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Item">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute use="required" type="xs:string" name="id" />
            <xs:attribute use="required" type="xs:string" name="value" />
            <xs:attribute type="xs:string" name="mappedValue" />
            <xs:attribute type="REAL-NUMBER" name="weight" />
        </xs:complexType>
    </xs:element>
    <xs:element name="Itemset">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="ItemRef" minOccurs="0" />
            </xs:sequence>
            <xs:attribute use="required" type="xs:string" name="id" />
            <xs:attribute type="PROB-NUMBER" name="support" />
            <xs:attribute type="xs:nonNegativeInteger" name="numberOfItems" />
        </xs:complexType>
    </xs:element>
    <xs:element name="ItemRef">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute use="required" type="xs:string" name="itemRef" />
        </xs:complexType>
    </xs:element>
    <xs:element name="AssociationRule">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute use="required" type="xs:string" name="antecedent" />
            <xs:attribute use="required" type="xs:string" name="consequent" />
            <xs:attribute use="required" type="PROB-NUMBER" name="support" />
            <xs:attribute use="required" type="PROB-NUMBER" name="confidence" />
            <xs:attribute use="optional" type="xs:float" name="lift" />
            <xs:attribute use="optional" type="xs:float" name="leverage" />
            <xs:attribute use="optional" type="PROB-NUMBER" name="affinity" />
            <xs:attribute use="optional" type="xs:string" name="id" />
        </xs:complexType>
    </xs:element>
    <xs:element name="TextModel">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="MiningSchema" />
                <xs:element ref="Output" minOccurs="0" />
                <xs:element ref="ModelStats" minOccurs="0" />
                <xs:element ref="ModelExplanation" minOccurs="0" />
                <xs:element ref="Targets" minOccurs="0" />
                <xs:element ref="LocalTransformations" minOccurs="0" />
                <xs:element ref="TextDictionary" />
                <xs:element ref="TextCorpus" />
                <xs:element ref="DocumentTermMatrix" />
                <xs:element ref="TextModelNormalization" minOccurs="0" />
                <xs:element ref="TextModelSimiliarity" minOccurs="0" />
                <xs:element ref="ModelVerification" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute type="xs:string" name="modelName" />
            <xs:attribute use="required" type="MINING-FUNCTION" name="functionName" />
            <xs:attribute type="xs:string" name="algorithmName" />
            <xs:attribute use="required" type="xs:integer" name="numberOfTerms" />
            <xs:attribute use="required" type="xs:integer" name="numberOfDocuments" />
            <xs:attribute default="true" type="xs:boolean" name="isScorable" />
        </xs:complexType>
    </xs:element>
    <xs:element name="TextDictionary">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="Taxonomy" minOccurs="0" />
                <xs:group ref="STRING-ARRAY" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="TextCorpus">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element maxOccurs="unbounded" ref="TextDocument" minOccurs="0" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="TextDocument">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute use="required" type="xs:string" name="id" />
            <xs:attribute use="optional" type="xs:string" name="name" />
            <xs:attribute use="optional" type="INT-NUMBER" name="length" />
            <xs:attribute use="optional" type="xs:string" name="file" />
        </xs:complexType>
    </xs:element>
    <xs:element name="DocumentTermMatrix">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="Matrix" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="TextModelNormalization">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute default="termFrequency" name="localTermWeights">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="termFrequency" />
                        <xs:enumeration value="binary" />
                        <xs:enumeration value="logarithmic" />
                        <xs:enumeration value="augmentedNormalizedTermFrequency" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute default="inverseDocumentFrequency" name="globalTermWeights">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="inverseDocumentFrequency" />
                        <xs:enumeration value="none" />
                        <xs:enumeration value="GFIDF" />
                        <xs:enumeration value="normal" />
                        <xs:enumeration value="probabilisticInverse" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute default="none" name="documentNormalization">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="none" />
                        <xs:enumeration value="cosine" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="TextModelSimiliarity">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute name="similarityType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="euclidean" />
                        <xs:enumeration value="cosine" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="ModelExplanation">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:choice>
                    <xs:element maxOccurs="unbounded" ref="PredictiveModelQuality" minOccurs="0" />
                    <xs:element maxOccurs="unbounded" ref="ClusteringModelQuality" minOccurs="0" />
                </xs:choice>
                <xs:element ref="Correlations" minOccurs="0" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="PredictiveModelQuality">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="ConfusionMatrix" minOccurs="0" />
                <xs:element ref="LiftData" minOccurs="0" />
                <xs:element ref="ROC" minOccurs="0" />
            </xs:sequence>
            <xs:attribute use="required" type="xs:string" name="targetField" />
            <xs:attribute use="optional" type="xs:string" name="dataName" />
            <xs:attribute default="training" name="dataUsage">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="training" />
                        <xs:enumeration value="test" />
                        <xs:enumeration value="validation" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute use="optional" type="NUMBER" name="meanError" />
            <xs:attribute use="optional" type="NUMBER" name="meanAbsoluteError" />
            <xs:attribute use="optional" type="NUMBER" name="meanSquaredError" />
            <xs:attribute use="optional" type="NUMBER" name="rootMeanSquaredError" />
            <xs:attribute use="optional" type="NUMBER" name="r-squared" />
            <xs:attribute use="optional" type="NUMBER" name="adj-r-squared" />
            <xs:attribute use="optional" type="NUMBER" name="sumSquaredError" />
            <xs:attribute use="optional" type="NUMBER" name="sumSquaredRegression" />
            <xs:attribute use="optional" type="NUMBER" name="numOfRecords" />
            <xs:attribute use="optional" type="NUMBER" name="numOfRecordsWeighted" />
            <xs:attribute use="optional" type="NUMBER" name="numOfPredictors" />
            <xs:attribute use="optional" type="NUMBER" name="degreesOfFreedom" />
            <xs:attribute use="optional" type="NUMBER" name="fStatistic" />
            <xs:attribute use="optional" type="NUMBER" name="AIC" />
            <xs:attribute use="optional" type="NUMBER" name="BIC" />
            <xs:attribute use="optional" type="NUMBER" name="AICc" />
        </xs:complexType>
    </xs:element>
    <xs:element name="ClusteringModelQuality">
        <xs:complexType>
            <xs:attribute use="optional" type="xs:string" name="dataName" />
            <xs:attribute use="optional" type="NUMBER" name="SSE" />
            <xs:attribute use="optional" type="NUMBER" name="SSB" />
        </xs:complexType>
    </xs:element>
    <xs:element name="LiftData">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="ModelLiftGraph" />
                <xs:element ref="OptimumLiftGraph" minOccurs="0" />
                <xs:element ref="RandomLiftGraph" minOccurs="0" />
            </xs:sequence>
            <xs:attribute type="xs:string" name="targetFieldValue" />
            <xs:attribute type="xs:string" name="targetFieldDisplayValue" />
            <xs:attribute type="NUMBER" name="rankingQuality" />
        </xs:complexType>
    </xs:element>
    <xs:element name="ModelLiftGraph">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="LiftGraph" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="OptimumLiftGraph">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="LiftGraph" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="RandomLiftGraph">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="LiftGraph" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="LiftGraph">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="XCoordinates" />
                <xs:element ref="YCoordinates" />
                <xs:element ref="BoundaryValues" minOccurs="0" />
                <xs:element ref="BoundaryValueMeans" minOccurs="0" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="XCoordinates">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:group ref="NUM-ARRAY" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="YCoordinates">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:group ref="NUM-ARRAY" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="BoundaryValues">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:group ref="NUM-ARRAY" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="BoundaryValueMeans">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:group ref="NUM-ARRAY" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ROC">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="ROCGraph" />
            </xs:sequence>
            <xs:attribute use="required" type="xs:string" name="positiveTargetFieldValue" />
            <xs:attribute type="xs:string" name="positiveTargetFieldDisplayValue" />
            <xs:attribute type="xs:string" name="negativeTargetFieldValue" />
            <xs:attribute type="xs:string" name="negativeTargetFieldDisplayValue" />
        </xs:complexType>
    </xs:element>
    <xs:element name="ROCGraph">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="XCoordinates" />
                <xs:element ref="YCoordinates" />
                <xs:element ref="BoundaryValues" minOccurs="0" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ConfusionMatrix">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="ClassLabels" />
                <xs:element ref="Matrix" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="ClassLabels">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:group ref="STRING-ARRAY" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="Correlations">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:element ref="CorrelationFields" />
                <xs:element ref="CorrelationValues" />
                <xs:element ref="CorrelationMethods" minOccurs="0" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="CorrelationFields">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
                <xs:group ref="STRING-ARRAY" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
