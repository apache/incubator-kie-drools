<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.drools</groupId>
    <artifactId>drools</artifactId>
    <version>7.33.0-SNAPSHOT</version>
  </parent>

  <artifactId>kie-pmml-refactoring</artifactId>
  <packaging>pom</packaging>

  <name>KIE :: PMML</name>
  <description>Support for PMML-Encoded Predictive Models</description>

  <modules>
    <module>kie-pmml-library</module>
    <module>kie-pmml-models</module>
    <module>kie-pmml-runtime</module>
    <module>kie-pmml-api</module>
    <module>kie-pmml-integration-tests</module>
  </modules>

  <properties>
    <version.blueprinter>1.0</version.blueprinter>
  </properties>

  <build>
    <plugins>
      <plugin>
        <groupId>org.kie</groupId>
        <artifactId>blueprinter-maven-plugin</artifactId>
      </plugin>
    </plugins>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.kie</groupId>
          <artifactId>blueprinter-maven-plugin</artifactId>
          <version>${version.blueprinter}</version>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>


</project>
