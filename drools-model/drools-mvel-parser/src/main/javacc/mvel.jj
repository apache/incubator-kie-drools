        type = ClassOrInterfaceType(annotations)
        (
            LOOKAHEAD(Annotations() "[")
            arrayBracketPair = ArrayBracketPair(Origin.TYPE) { arrayBracketPairs=add(arrayBracketPairs, arrayBracketPair); }
