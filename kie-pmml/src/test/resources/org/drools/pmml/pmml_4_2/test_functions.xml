<PMML version="4.2" xsi:schemaLocation="http://www.dmg.org/PMML-4_2 http://www.dmg.org/v4-1/pmml-4-2.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.dmg.org/PMML-4_2">
  <Header copyright="JBoss"/>
  <DataDictionary>
    <DataField dataType="integer" name="Age" optype="continuous">
      <Value property="missing" value="-1"/>
    </DataField>
    <DataField dataType="integer" name="Age2" optype="continuous">
      <Value property="missing" value="-1"/>
    </DataField>
  </DataDictionary>
  <TransformationDictionary>
    <DerivedField dataType="double" name="NestedAge" optype="continuous">
      <Apply function="floor">
        <Apply function="sum">
          <Apply function="ln">
            <Apply function="sqrt">
              <Apply function="pow">
                <Constant dataType="double">10.0</Constant>
                <Constant dataType="integer">2</Constant>
              </Apply>
            </Apply>
          </Apply>
          <Apply function="pow">
            <FieldRef field="Age"/>
            <FieldRef field="Age2"/>
          </Apply>
          <FieldRef field="Age"/>
        </Apply>
      </Apply>
    </DerivedField>
    <DerivedField dataType="integer" name="MappedAge" optype="continuous">
      <Apply function="sum">
        <Constant dataType="integer">10</Constant>
        <Constant dataType="integer">20</Constant>
      </Apply>
    </DerivedField>
  </TransformationDictionary>
</PMML>