<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN"
    "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module>

  <!-- Here are included all required classes for manage DMN Feel type in a client GWT environment  -->
  <!-- PLEASE NOTE! BuiltInType.class contains type NOT compatible with GWT eg. new java Time api   -->
  <!-- Therefore, when importing this module in your GWT project, you must substitute BuiltInType   -->
  <!-- with a specific class compatible with GWT. To substitute it, just insert in your GWT project -->
  <!-- a new BuiltInType.java class in a "org.kie.dmn.feel.lang.type" defined package.              -->
  <source path="lang" includes="**/CompositeType.java"/>
  <source path="lang" includes="**/Scope.java"/>
  <source path="lang" includes="**/SimpleType.java"/>
  <source path="lang" includes="**/Symbol.java"/>
  <source path="lang" includes="**/Type.java"/>
  <source path="lang/impl" includes="**/MapBackedType.java"/>
  <source path="lang/types" includes="**/BaseSymbol.java"/>
  <source path="lang/types" includes="**/BuiltInType.java"/>
  <source path="lang/types" includes="**/BuiltInTypeSymbol.java"/>

</module>
