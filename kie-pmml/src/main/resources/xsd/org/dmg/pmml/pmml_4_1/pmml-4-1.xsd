            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="Extension" minOccurs="0" />
            </xs:sequence>
            <xs:attribute use="required" type="FIELD-NAME" name="field" />
            <xs:attribute use="required" name="value" />
            <xs:attribute use="required" type="NUMBER" name="count" />
        </xs:complexType>
    </xs:element>
