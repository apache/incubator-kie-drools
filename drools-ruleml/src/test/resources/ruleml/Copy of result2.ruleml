<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RuleML  xsi:schemaLocation="http://svn.codehaus.org/logicabyss/RuleML2Drools/trunk/resources/rrml/pr.xsd">
	<Rule>
		<if>
			<And>
				<Atom>
					<op>
						<Rel>Buy</Rel>
					</op>
					<slot>
						<Ind>buyer</Ind>
						<Ind>Ti6o</Ind>
					</slot>
					<slot>
						<Ind>seller</Ind>
						<Var />
					</slot>
					<slot>
						<Ind>item</Ind>
						<Ind>Objective</Ind>
					</slot>
				</Atom>
				<Atom>
					<op>
						<Rel>Person</Rel>
					</op>
					<slot>
						<Ind>name</Ind>
						<Ind>Ti6o</Ind>
					</slot>
					<slot>
						<Ind>age</Ind>
						<Var />
					</slot>
				</Atom>
			</And>
		</if>
		<do>
			<Retract>
				<Atom>
					<op>
						<Rel>Buy</Rel>
					</op>
					<slot>
						<Ind>buyer</Ind>
						<Ind>Ti6o</Ind>
					</slot>
					<slot>
						<Ind>seller</Ind>
						<Var />
					</slot>
					<slot>
						<Ind>item</Ind>
						<Ind>Objective</Ind>
					</slot>
				</Atom>
			</Retract>
		</do>
	</Rule>
</RuleML>

