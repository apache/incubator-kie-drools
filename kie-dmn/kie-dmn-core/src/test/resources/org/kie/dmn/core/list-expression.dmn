<definitions id="_list-expression" name="list-expression"
	namespace="https://github.com/kiegroup/kie-dmn"
	xmlns="http://www.omg.org/spec/DMN/20151101/dmn.xsd"
	xmlns:feel="http://www.omg.org/spec/FEEL/20140401">
	<decision name="Name list" id="d_NameList">
		<variable name="Name list" typeRef="feel:list"/>
		<list>
			<literalExpression><text>"John"</text></literalExpression>
			<literalExpression><text>"Ma"+"ry"</text></literalExpression>
		</list>
	</decision>
</definitions>
