package org.kie.pmml.compiler.commons;

import java.util.Arrays;
import java.util.Collections;

import org.kie.pmml.commons.model.KiePMMLTargetValue;

public class KiePMMLTargetValueTemplate {

    private KiePMMLTargetValue getKiePMMLTargetValue() {
        KiePMMLTargetValue targetValue = KiePMMLTargetValue.builder(name, Collections.emptyList())
                .withValue(value)
                .withDisplayValue(displayValue)
                .withPriorProbability(priorProbability)
                .withDefaultValue(defaultValue)
                .build();
    }

}
