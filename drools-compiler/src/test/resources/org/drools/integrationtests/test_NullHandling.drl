package org.drools.test;
 
import org.drools.Cheese;
import org.drools.Person;

global java.util.List list;
 
rule "simple rule"
    when
        Cheese( type == null )
    then
		list.add( new Integer(5) ); 
end 

rule "another rule"
	when
		Person( bigDecimal != null )
    then 
    	list.add( new Integer(6) );
end    	