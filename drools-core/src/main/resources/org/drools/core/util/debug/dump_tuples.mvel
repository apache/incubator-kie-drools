============== Memory Data Info ==================
@code{
    java.util.Iterator it = session.session.objectStore.iterateFactHandles();
    buffer = new StringBuilder();
    for( int i = 0; i < 150 && it.hasNext(); i++ ) {
        ifh = it.next();
        buffer.append( i );
        buffer.append( ". " );
        buffer.append( ifh.toTupleTree(4) );
        buffer.append( "\n" );
    }
}
