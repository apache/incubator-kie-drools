<kie-server-state>
  <controllers/>
  <configuration>
    <configItems>
      <config-item>
        <name>org.kie.server.location</name>
        <value>http://${appServerAddress}:${appServerPort}/rest/server</value>
        <type>java.lang.String</type>
      </config-item>
    </configItems>
  </configuration>
  <containers>
    <container>
      <containerId>KJAR_CONTAINER_ID_MARKER</containerId>
      <releaseId>
        <groupId>${kjarGroupId}</groupId>
        <artifactId>${kjarArtifactId}</artifactId>
        <version>${kjarVersion}</version>
      </releaseId>
      <status>STARTED</status>
      <scanner>
        <status>STOPPED</status>
      </scanner>
      <configItems>
        <config-item>
          <name>KBase</name>
          <value></value>
          <type>BPM</type>
        </config-item>
        <config-item>
          <name>KSession</name>
          <value></value>
          <type>BPM</type>
        </config-item>
        <config-item>
          <name>MergeMode</name>
          <value>MERGE_COLLECTIONS</value>
          <type>BPM</type>
        </config-item>
        <config-item>
          <name>RuntimeStrategy</name>
          <value>${runtimeStrategy}</value>
          <type>BPM</type>
        </config-item>
      </configItems>
      <messages/>
      <containerAlias>${kjarArtifactId}</containerAlias>
    </container>
  </containers>
</kie-server-state>