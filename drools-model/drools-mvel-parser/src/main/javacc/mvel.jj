    [ expr = Expression() ]
    ";"
    { return new ReturnStmt(range(begin, token()),expr); }
}

/**
 * https://docs.oracle.com/javase/specs/jls/se15/html/jls-14.html#jls-14.18
 * <pre>{@code
 *     ThrowStatement:
 *         throw Expression ;
 * }</pre>
 */
ThrowStmt ThrowStatement():
{
