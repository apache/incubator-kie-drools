package org.drools.test;

import org.drools.Cheese;
import java.util.List;
import java.util.Set;

global List results;

rule "External Function" salience 80
    when
        $set  : Set( size >= 3 )
                from accumulate( Cheese( $type : type ),
                                 collectSet( $type ) );
    then
        results.add( $set );
end  
